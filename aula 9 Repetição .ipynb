   "source": [
    "n = int(input(\"Digite um número: \"))\n",
    "ehprimo = True \n",
    "i = 2\n",
    "while i < n:\n",
    "    if (n % i) == 0:\n",
    "        ehprimo = False\n",
    "    i = i + 1\n",
    "\n",
    "if ehprimo:\n",
    "    print(\"É primo\")\n",
    "else: \n",
    "    print(\"Não é primo\")"
   ]
